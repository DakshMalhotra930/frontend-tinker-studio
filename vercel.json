{
  "version": 2,
  "builds": [
    {
      "src": "frontend-tinker-studio/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend-tinker-studio/$1"
    }
  ],
  "buildCommand": "cd frontend-tinker-studio && npm install && npm run build",
  "outputDirectory": "frontend-tinker-studio/dist",
  "installCommand": "cd frontend-tinker-studio && npm install"
}
